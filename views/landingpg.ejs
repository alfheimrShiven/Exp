          /*fq dropdowns*/
          
          .accordion a {
            position: relative;
            display: -webkit-box;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-orient: vertical;
            -webkit-box-direction: normal;
            -webkit-flex-direction: column;
            -ms-flex-direction: column;
            flex-direction: column;
            width: 100%;
            padding: 1rem 3rem 1rem 1rem;
            color: #4F8AFC;
            font-size: 1.15rem;
            font-weight: 400;
            border-bottom: 1px solid #e5e5e5;
          }

          .accordion a:hover,
          .accordion a:hover::after {
            cursor: pointer;
            color: #4F8AFC;
          }

          .accordion a:hover::after {
            border: 1px solid #4F8AFC;
          }

          .accordion a.active {
            color: #4F8AFC;
            border-bottom: 1px solid #4F8AFC;
          }

          .accordion a::after {   
            content: '+';
            position: absolute;
            float: left;
            right: 1rem;
            font-size: 1rem;
            color: #707070;
            font-size: xx-large;
            width: 30px;
            height: 30px;
            -webkit-border-radius: 50%;
            -moz-border-radius: 50%;
            border-radius: 50%;
            border: 1px solid #7288a2;
            text-align: center;
          }

          .accordion a.active::after {    
            content: '-';
            font-size: xx-large;
            color: #4F8AFC;
            border: 1px solid #4F8AFC;
          }

          .accordion .content {
            opacity: 0;
            padding: 0 1rem;
            max-height: 0;
            border-bottom: 1px solid #e5e5e5;
            overflow: hidden;
            clear: both;
            -webkit-transition: all 0.2s ease 0.15s;
            -o-transition: all 0.2s ease 0.15s;
            transition: all 0.2s ease 0.15s;
          }

          .accordion .content p {
            font-size: 1rem;
            font-weight: 300;
          }

          .accordion .content.active {
            opacity: 1;
            padding: 1rem;
            max-height: 100%;
            -webkit-transition: all 0.35s ease 0.15s;
            -o-transition: all 0.35s ease 0.15s;
            transition: all 0.35s ease 0.15s;
          }
          .content p{
            padding: 0;
          }
