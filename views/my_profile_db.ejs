<script>
// document.getElementById("saveUserDataBtn").onclick():
  
function savedata(){
  // checking gender
  let gender='';
  if(document.getElementById("maleradiobtn").checked)
    gender= 'M';
  else if(document.getElementById("femaleradiobtn").checked)
    gender= 'F';
  else
    gender= 'T';  
  // get all the values entered by the user
  let userDataJson= {
    "fname": document.getElementById("firstnameinput").value,
    "lname": document.getElementById("lastnameinput").value,
    "dob": document.getElementById("datepicker").value,
    "sex": gender,
    "phone": document.getElementById("phninput").value,
    "mail": document.getElementById("emailinput").value,
    "address": document.getElementById("streetinput").value,
    "city":document.getElementById("cityinput").value,
    "pin": document.getElementById("citypinInput").value
  }
  console.log("Sending this userJson:"+ JSON.stringify(userDataJson));
  // fetch req to /profilesave
  fetch("/user/profilesave",{
    method: "POST",
    headers: {
      "Content-Type": "application/json"
    },
    body: JSON.stringify(userDataJson)
  }).then((res) => {
    if(res.status.code == 409)
      alert("User already exists");
    else{
      alert("Your profile information has been saved");
      window.location= "/user/dashboard";
    }
  }).catch((error) => console.log("Error while storing profile info:" + error));
}

</script>



